import os

# Nama folder dan file yang akan dibuat
folder_name = 'Tabs'
files = {
    'home.py': "# Konten default untuk home.py",
    'prediction.py': "# Konten default untuk prediction.py"
}

# Membuat folder Tabs jika belum ada
os.makedirs(folder_name, exist_ok=True)

# Membuat setiap file di dalam folder Tabs
for file_name, content in files.items():
    file_path = os.path.join(folder_name, file_name)
    if not os.path.exists(file_path):
        with open(file_path, 'w') as f:
            f.write(content)
        print(f"File '{file_name}' telah dibuat di dalam folder '{folder_name}'.")
    else:
        print(f"File '{file_name}' sudah ada di dalam folder '{folder_name}'.")
